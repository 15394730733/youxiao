<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/item_selector_listViewRight_LL_TV_CMDTY_ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <RelativeLayout
            android:layout_width="@dimen/x300"
            android:layout_height="@dimen/x56"
            android:layout_gravity="center_vertical">

            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:singleLine="true"
                android:textSize="14sp"
                android:textColor="@color/grey800"
                android:hint="莲雾"
                android:layout_marginLeft="8dp"/>
        </RelativeLayout>

        <View
            android:layout_width="0.3dp"
            android:layout_height="match_parent"
            android:background="#ccc">

        </View>

        <RelativeLayout
            android:layout_width="@dimen/x140"
            android:layout_height="@dimen/x100"
            android:layout_gravity="center_vertical">


            <TextView
                android:id="@+id/item_selector_listViewRight_LL_ET_NonRetailCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginStart="5dp"
                android:hint="00"
                android:singleLine="true"
                android:gravity="start"/>


            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV_NonRetailUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_alignParentRight="true"
                android:text="箱"
                android:textSize="14sp" />
        </RelativeLayout>

        <View
            android:layout_width="0.3dp"
            android:layout_height="match_parent"
            android:background="#ccc">

        </View>

        <RelativeLayout
            android:layout_width="@dimen/x140"
            android:layout_height="@dimen/x100"
            android:layout_gravity="center_vertical">


            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV_RetailCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginStart="5dp"
                android:hint="00"
                android:singleLine="true"
                android:gravity="start"/>


            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV_RetailUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_alignParentRight="true"
                android:text="散"
                android:textSize="14sp"/>
        </RelativeLayout>

        <View
            android:layout_width="0.3dp"
            android:layout_height="match_parent"
            android:background="#ccc">

        </View>

        <RelativeLayout
            android:layout_width="@dimen/x220"
            android:layout_height="@dimen/x56"
            android:layout_gravity="center_vertical">

            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV_BOX_PRICE"
                android:layout_width="48dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:hint="00.00"
                android:singleLine="true" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginStart="5dp"
                android:layout_toEndOf="@id/item_selector_listViewRight_LL_TV_BOX_PRICE"
                android:layout_toRightOf="@id/item_selector_listViewRight_LL_TV_BOX_PRICE"
                android:text="箱价"
                android:textSize="14sp" />
        </RelativeLayout>

        <View
            android:layout_width="0.3dp"
            android:layout_height="match_parent"
            android:background="#ccc">

        </View>

        <RelativeLayout
            android:layout_width="@dimen/x220"
            android:layout_height="@dimen/x56"
            android:layout_gravity="center_vertical">

            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV_PRICE"
                android:layout_width="48dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:hint="00.00"
                android:singleLine="true" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginStart="5dp"
                android:layout_toEndOf="@id/item_selector_listViewRight_LL_TV_PRICE"
                android:layout_toRightOf="@id/item_selector_listViewRight_LL_TV_PRICE"
                android:text="单价"
                android:textSize="14sp" />
        </RelativeLayout>

        <View
            android:layout_width="0.3dp"
            android:layout_height="match_parent"
            android:background="#ccc">

        </View>

        <RelativeLayout
            android:layout_width="@dimen/x350"
            android:layout_height="@dimen/x56"
            android:layout_gravity="center_vertical">

            <TextView
                android:id="@+id/item_selector_listViewRight_LL_TV_PRODUCTION_DT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:background="@null"
                android:digits="1234567890-"
                android:hint="0000-00-00"
                android:singleLine="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginStart="5dp"
                android:layout_toEndOf="@id/item_selector_listViewRight_LL_TV_PRODUCTION_DT"
                android:layout_toRightOf="@id/item_selector_listViewRight_LL_TV_PRODUCTION_DT"
                android:text="生产日期"
                android:textSize="14sp" />
        </RelativeLayout>


    </LinearLayout>


</LinearLayout>
